{
	"name": "boilerplate-angular",
	"version": "0.0.1",
	"author": "Youssef Ben Sadik",
	"scripts": {
		"ng": "ng",
		"start": "bun ./scripts.ts && ng serve",
		"build": "bun ./scripts.ts && ng build",
		"watch": "bun ./scripts.ts && ng build --watch --configuration development",
		"test": "bun ./scripts.ts && ng test",
		"serve:ssr:boilerplate-angular": "bun ./scripts.ts && node dist/boilerplate-angular/server/server.mjs",
		"lint": "bun ./scripts.ts && ng lint"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^18.2.0",
		"@angular/common": "^18.2.0",
		"@angular/compiler": "^18.2.0",
		"@angular/core": "^18.2.0",
		"@angular/forms": "^18.2.0",
		"@angular/platform-browser": "^18.2.0",
		"@angular/platform-browser-dynamic": "^18.2.0",
		"@angular/platform-server": "^18.2.0",
		"@angular/router": "^18.2.0",
		"@angular/ssr": "^18.2.7",
		"@ngx-translate/core": "^15.0.0",
		"@ngx-translate/http-loader": "^8.0.0",
		"express": "^4.18.2",
		"ngx-logger": "^5.0.12",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zod": "^3.23.8",
		"zone.js": "~0.14.10"
