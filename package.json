		"@eslint/js": "^9.12.0",
		"@types/express": "^4.17.17",
		"@types/jasmine": "~5.1.0",
		"@types/node": "^18.18.0",
		"angular-eslint": "18.3.1",
		"autoprefixer": "^10.4.20",
		"bun": "^1.2.5",
		"eslint": "^9.9.1",
		"hygen": "^6.2.11",
		"jasmine-core": "~5.2.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"postcss": "^8.4.47",
